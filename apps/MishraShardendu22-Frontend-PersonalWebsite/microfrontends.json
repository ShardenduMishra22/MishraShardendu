{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "ms22-main": {
      "development": {
        "local": 3000,
        "fallback": "ms22-main.vercel.app"
      }
    },
    "ms22-blog": {
      "development": {
        "local": 5173,
        "fallback": "ms22-blog.vercel.app"
      },
      "routing": [
        {
          "group": "admin",
          "paths": [
            "/admin",
            "/admin/login",
            "/admin/dashboard",
            "/admin/profile",
            "/admin/projects",
            "/admin/experiences",
            "/admin/skills",
            "/admin/certifications",
            "/admin/kanban",
            "/admin/:path*",
            "/vc-ap-ms22-blog/:path*"
          ]
        }
      ]
    }
  }
}
